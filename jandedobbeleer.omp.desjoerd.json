{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffc107",
          "properties": {
            "postfix": " ",
            "display_host": false
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "background": "#18354c",
          "foreground": "#ffc107",
          "properties": {
            "prefix": "<transparent></> ",
            "home_icon": "\uF7DB",
            "enable_hyperlink": true,
            "folder_icon": "\uF115",
            "folder_separator_icon": " \uE0B1 ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#d2ff5e",
          "properties": {
            "display_stash_count": true,

            "status_colors_enabled": true,
            "display_status": true,
            "local_changes_color": "#ff9248",
            "ahead_and_behind_color": "#f26d50",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#111111",
          "background": "#ffff66",
          "properties": {
            "root_icon": ""
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffc107",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_exit_code": false,
            "always_enabled": true,
            "error_color": "#f1184c",
            "color_background": true,
            "prefix": "<transparent></> "
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "background": "#18354c",
          "foreground": "#ffc107",
          "properties": {
            "prefix": "<#18354c,transparent></>  "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "trailing_diamond": "",
          "background": "#18354c",
          "foreground": "#ffc107"
        }
      ]
    }
  ],
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}"
}
